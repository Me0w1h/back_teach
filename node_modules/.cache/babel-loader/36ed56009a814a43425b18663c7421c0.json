ssNames.length; index++) {\n        var element = this.classNames[index].value;\n\n        if (element === this.addUser.classId) {\n          this.addUser.className = this.classNames[index].label;\n        }\n      }\n\n      axios.post(\"http://127.0.0.1/user/1\", this.addUser).then(function (res) {\n        _this3.dialogVisible = false;\n        _this3.addUser.id = '', _this3.addUser.name = '', _this3.addUser.username = '', _this3.addUser.className = '', _this3.addUser.classId = '', _this3.addUser.school = '', _this3.addUser.exam = '';\n\n        if (res.data.code === 20011) {\n          _this3.$message.success('添加成功');\n\n          _this3.selectAll();\n        } else if (res.data.code === 20010) {\n          _this3.$message.error('添加失败');\n        } else if (res.data.code === 20031) {\n          _this3.$message.success('修改成功');\n\n          _this3.selectAll();\n        } else if (res.data.code === 20030) {\n          _this3.$message.error('修改失败');\n        }\n      });\n    },\n    updateById: function updateById(row) {\n      this.addUser.id = row.id;\n      this.addUser.name = row.name;\n      this.addUser.classId = row.classId;\n      this.addUser.username = row.username;\n      this.addUser.className = row.className;\n      this.addUser.school = row.school;\n      this.addUser.exam = row.exam;\n      this.dialogVisible2 = true;\n    },\n    deleteById: function deleteById(row) {\n      this.deleteIds[0] = row.id;\n      this.deleteuser();\n    },\n    deleteuser: function deleteuser() {\n      var _this4 = this;\n\n      axios.delete(\"http://127.0.0.1/user/\", {\n        data: this.deleteIds\n      }).then(function (res) {\n        if (res.data.code === 20021) {\n          _this4.$message.success('删除成功');\n        } else {\n          _this4.$message.error('删除失败');\n        }\n      });\n      this.deleteIds = [];\n      this.selectAll();\n    },\n    handleSelectionChange: function handleSelectionChange(val) {\n      // window.alert(val.username)\n      this