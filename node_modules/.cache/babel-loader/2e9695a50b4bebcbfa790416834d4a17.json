p://127.0.0.1/user/?currentPage=\" + this.currentPage + \"&size=\" + this.pageSize, this.searchUser).then(function (res) {\n        _this3.alluser = res.data.data.rows;\n        _this3.totalCount = Number(res.data.data.totalCount);\n      });\n    },\n    onSubmit: function onSubmit() {\n      this.selectAll();\n    },\n    //分页\n    handleSizeChange: function handleSizeChange(val) {\n      this.pageSize = val;\n      this.selectAll();\n    },\n    handleCurrentChange: function handleCurrentChange(val) {\n      // console.log(`当前页: ${val}`);\n      this.currentPage = val;\n      this.selectAll();\n    },\n    addaUser: function addaUser() {\n      var _this4 = this;\n\n      for (var index = 0; index < this.classNames.length; index++) {\n        var element = this.classNames[index].value;\n\n        if (element === this.addUser.classId) {\n          this.addUser.className = this.classNames[index].label;\n        }\n      }\n\n      axios.post(\"http://127.0.0.1/user/1\", this.addUser).then(function (res) {\n        _this4.dialogVisible = false;\n        _this4.addUser.id = '', _this4.addUser.name = '', _this4.addUser.username = '', _this4.addUser.className = '', _this4.addUser.classId = '', _this4.addUser.school = '', _this4.addUser.exam = '';\n\n        if (res.data.code === 20011) {\n          _this4.$message.success('添加成功');\n\n          _this4.selectAll();\n        } else if (res.data.code === 20010) {\n          _this4.$message.error('添加失败');\n        } else if (res.data.code === 20031) {\n          _this4.$message.success('修改成功');\n\n          _this4.selectAll();\n        } else if (res.data.code === 20030) {\n          _this4.$message.error('修改失败');\n        }\n      });\n    },\n    updateById: function updateById(row) {\n      this.addUser.id = row.id;\n      this.addUser.name = row.name;\n      this.addUser.classId = row.classId;\n      this.addUser.username = row.username;\n      this.addUser.className = row.className;\n      this.addUser.school = row.school;\n      this.addUser.exam = row.exam;\n      this.dialogVisible2 = true;\n    },\n    deleteById: function deleteById(row) {\n      this.deleteIds[0] = row.id;\n      this.deleteuser();\n    },\n    deleteuser: function deleteuser() {\n      var _this5 = this;\n\n      axios.delete(\"http://127.0.0.1/user/\", {\n        data: this.deleteIds\n      }).then(function (res) {\n        if (res.data.code === 20021) {\n          _this5.$message.success('删除成功');\n        } else {\n          _this5.$message.error('删除失败');\n        }\n      });\n      this.deleteIds = [];\n      this.selectAll();\n    },\n    handleSelectionChange: function handleSelectionChange(val) {\n      this.multipleSelection = val;\n    },\n    deleteByIds: function deleteByIds() {\n      if (this.multipleSelection.length > 0) {\n        for (var index = 0; index < this.multipleSelection.length; index++) {\n          window.alert(this.multipleSelection[index].id);\n          this.deleteIds[index] = this.multipleSelection[index].id;\n        }\n\n        this.deleteuser();\n      } else {\n        this.$message.error('请选择数据');\n      }\n\n      this.deleteIds = [];\n    },\n    addClass: function addClass() {\n      var _this6 = this;\n\n      axios.post(\"http://localhost/class\", this.addClassData).then(function (res) {\n        _this6.addClassVisible = false;\n        _this6.addClassData.className = '', _this6.addClassData.classId = '', _this6.addClassData.school = '';\n\n        if (res.data.code === 20011) {\n          _this6.$message.success('添加成功');\n        } else if (res.data.code === 20010) {\n          _this6.$message.error('添加失败');\n        } else if (res.data.code === 20031) {\n          _this6.$message.success('修改成功');\n        } else if (res.data.code === 20030) {\n          _this6.$message.error('修改失败');\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;