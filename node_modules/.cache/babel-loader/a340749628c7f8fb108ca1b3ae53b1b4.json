[x].school,\r\n                        children: [{\r\n                          value:res.data.data[x].classId,\r\n                          label: res.data.data[x].className}]\r\n                          })}\r\n                      \r\n                      \r\n                    }  \r\n                })},\r\n                },\r\n            // 添加考试\r\n        handleaddExam(){\r\n          \r\n                axios.post(\"http://172.27.206.137/exams\",this.addExam).then(res => {\r\n                    this.ExamlogVisible = false;  \r\n                    if (res.data.code==20011) {\r\n                      this.$message.success('添加成功');\r\n                      this.selectAll();\r\n                    }  else if(res.data.code==20010){\r\n                      this.$message.error('添加失败');\r\n                    }\r\n                    this.addExam.examName = '',   \r\n                    this.addExam.description = '', \r\n                    this.addExam.date = '', \r\n                    this.addExam.status = ''                                            \r\n                })  \r\n            },\r\n            // 查询考试的详细数据\r\n        selectClearData(row) {\r\n                \r\n                axios.get(\"http://172.27.206.137/texam/\"+row.id).then(res => { \r\n                    this.clearData = res.data.data; \r\n                })\r\n            },\r\n\r\n\r\n\r\n            // 查询班级数据\r\n    // getPlatCategory () {\r\n    //   // 请求顶级分类\r\n    //   axios.get('http://172.25.148.89/class')\r\n    //     .then(res => {\r\n    //       if (res.status === 200) {\r\n    //         console.log(123)\r\n    //         this.platOptions = res.data.data\r\n    //         this.platOptions.map((item, index, array) => {\r\n    //           // 因为数组和对象更新后不会更新视图，这里必须用$set方法\r\n    //           this.$set(array[index], 'child', [])\r\n    //         })\r\n    //       }\r\n    //     }).catch(error => {\r\n    //       let langmsg = this.$i18n.locale === 'zh' ? error.data