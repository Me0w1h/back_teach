se;\n\n        if (res.data.code == 20011) {\n          _this2.$message.success('添加成功');\n\n          _this2.selectAll();\n        } else if (res.data.code == 20010) {\n          _this2.$message.error('添加失败');\n        }\n\n        _this2.addExam.examName = '', _this2.addExam.description = '', _this2.addExam.date = '', _this2.addExam.status = '';\n      });\n    },\n    // 查询考试的详细数据\n    selectClearData: function selectClearData(row) {\n      var _this3 = this;\n\n      axios.get(\"http://172.25.148.89/texam/\" + row.id).then(function (res) {\n        _this3.clearData = res.data.data;\n      });\n    },\n    // 查询班级数据\n    getPlatCategory: function getPlatCategory() {\n      var _this4 = this;\n\n      // 请求顶级分类\n      axios.get('http://172.25.148.89/class').then(function (res) {\n        if (res.status === 200) {\n          console.log(123);\n          _this4.platOptions = res.data.data;\n\n          _this4.platOptions.map(function (item, index, array) {\n            // 因为数组和对象更新后不会更新视图，这里必须用$set方法\n            _this4.$set(array[index], 'child', []);\n          });\n        }\n      }).catch(function (error) {\n        var langmsg = _this4.$i18n.locale === 'zh' ? error.data.zhmsg : error.data.enmsg;\n\n        _this4.$message.error(langmsg);\n      });\n    } // handleItemChange (value) {\n    //   // 动态/异步加载分类数据\n    //   let parentId\n    //   if (value.length === 1) {\n    //     // 如果点击的是一级分类\n    //     parentId = value[0]\n    //     this.platOptions.map((item, index) => {\n    //       if (item.id === parentId && item.child.length === 0) {\n    //         // 当顶级分类的的child为空时才请求数据\n    //         this.$axios.get('/api/getCategorySon', {\n    //           params: {\n    //             parent_id: parentId\n    //           }\n    //         }).then(res => {\n    //           if (res.status === 200) {\n    //             this.$set(this.platOptions[index], 'child', res.data.data)\n    //             item.child.map((innerItem, innerIndex) => {\n    //              