.$message.error('删除失败');\n        }\n      });\n      this.deleteIds = [];\n      this.selectAll();\n    },\n\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n    },\n\n    deleteByIds() {\n      if (this.multipleSelection.length > 0) {\n        for (let index = 0; index < this.multipleSelection.length; index++) {\n          this.deleteIds[index] = this.multipleSelection[index].id;\n        }\n\n        this.deleteuser();\n      } else {\n        this.$message.error('请选择数据');\n      }\n\n      this.deleteIds = [];\n    },\n\n    addClass() {\n      axios.post(\"http://101.42.232.181:8090/class\", this.addClassData).then(res => {\n        this.addClassVisible = false;\n        this.addClassData.className = '', this.addClassData.classId = '', this.addClassData.school = '';\n\n        if (res.data.code === 20011) {\n          this.$message.success('添加成功');\n        } else if (res.data.code === 20010) {\n          this.$message.error('添加失败');\n        } else if (res.data.code === 20031) {\n          this.$message.success('修改成功');\n        } else if (res.data.code === 20030) {\n          this.$message.error('修改失败');\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiLAA;AACA;AACA;AACA;AACA;EACAC,YADA;;EAEAC;IACA;MACA;MACAC;QACAC,MADA;QAEAH,QAFA;QAGAI,YAHA;QAIAC,WAJA;QAKAC,aALA;QAMAC;MANA,CAFA;MAWA;MACAC,eAZA;MAcAC,aAdA;MAeAC,cAfA;MAgBAC,YAhBA;MAmBA;MACAC,oBApBA;MAqBAC,qBArBA;MAuBAC;QACAX,MADA;QAEAC,YAFA;QAGAW,YAHA;QAIAf,QAJA;QAKAM,aALA;QAMAD,WANA;QAOAE;MAPA,CAvBA;MAiCAS;QACAb,MADA;QAEAC,YAFA;QAGAJ,QAHA;QAIAK,WAJA;QAKAC,aALA;QAMAC;MANA,CAjCA;MA0CAU,sBA1CA;MA2CAC;QACAb,WADA;QAEAC,aAFA;QAGAC;MAHA,CA3CA;MAkDAY;QAEAnB,QAFA;QAGAM,aAHA;QAIAC;MAJA,CAlDA;MA2DAa,cA3DA;MA6DAC,WA7DA;MAmEA;MACAC,qBApEA;MAqEAC;IArEA;EAwEA,CA3EA;;EA4EAC;IACA;IACA;IACA;EAGA,CAlFA;;EAm